<h2>管理者ログイン</h2>

<%= form_with model: @admin do |f| %>
  <div class="field">
    <%= f.label :メールアドレス %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :パスワード %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  
    <%= f.submit "ログイン" %>
  
<% end %>

<%= render "admin/shared/links" %>
